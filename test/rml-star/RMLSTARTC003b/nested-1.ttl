@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/> .
@base <http://example.org/> .
:firstTM a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "test/rml-star/RMLSTARTC003b/data1.csv";
        rml:referenceFormulation ql:CSV
    ];
    rml:subjectMap [
        rr:template "http://example/{c1-1}"
    ];
    rr:predicateObjectMap [
        rr:predicate ex:p ;
        rml:objectMap [
            rr:template "http://example/{c1-2}"
        ]
    ] .

:secondTM a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "test/rml-star/RMLSTARTC003b/data1.csv";
        rml:referenceFormulation ql:CSV
    ];
    rml:subjectMap [
        rml:quotedTriplesMap :firstTM
    ];
    rr:predicateObjectMap [
        rr:predicate ex:r ;
        rml:objectMap [
            rr:template "http://example/{c1-3}"
        ]
    ] .

:thirdTM a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "test/rml-star/RMLSTARTC003b/data2.csv";
        rml:referenceFormulation ql:CSV
    ];
    rml:subjectMap [
        rml:quotedTriplesMap :secondTM;
        rr:joinCondition [
          rr:child "c2-2" ;
          rr:parent "c1-4" ;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:q ;
        rml:objectMap [
            rml:reference "c2-1" ;
            rr:datatype xsd:integer
        ]
    ] .
