@prefix ex: <http://example.com/> .
@prefix rml1: <http://w3id.org/rml/> .

ex:TriplesMap1 a rml1:TriplesMap ;
    rml1:logicalSource [ rml1:referenceFormulation <http://semweb.mmlab.be/ns/ql#CSV> ;
            rml1:source "data.csv" ] ;
    rml1:predicateObjectMap [ rml1:graphMap [ rml1:constant rml1:defaultGraph ;
                    rml1:termType rml1:IRI ] ;
            rml1:objectMap [ rml1:reference "name" ;
                    rml1:termType rml1:Literal ] ;
            rml1:predicateMap [ rml1:constant ex:name ;
                    rml1:termType rml1:IRI ] ] ;
    rml1:subjectMap [ rml1:template "http://example.com/{id}" ;
            rml1:termType rml1:IRI ] .

